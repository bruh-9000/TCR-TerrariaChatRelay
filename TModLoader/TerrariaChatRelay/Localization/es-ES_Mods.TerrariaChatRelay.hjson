	EndpointModConfig: {
		Tooltip: ""

		BotToken: {
			Label: Token del bot
			Tooltip: Para aprender como obtener el token de tu bot, lee la Guía del Token de Discord que se encuentra arriba
		}

		Channel_IDs: {
			Label: Identificación del canal
			Tooltip: Identificación del canal de Discord al que quieres enviar o recibir mensajes
		}

		Console_Channel_IDs: {
			Label: Identificación del canal para consola
			Tooltip: Identificación del canal de Discord al que quieres enviar or recibir los registros y mensajes de la consola
		}

		DenySendingMessagesToGame: {
			Label: Identificación del canal - No enviar
			Tooltip: Identificación del canal de Discord desde el que no quieras enviar mensajes a Terraria
		}

		DenyReceivingMessagesFromGame: {
			Label: Identificación del canal - No recibir
			Tooltip: Identificación del canal de Discord desde el que no quieras recibir mensajes de Terraria
		}

		HideMessagesWithString: {
			Label: Si se encuentra, esconder el mensaje
			Tooltip:
				'''
				Si cualquiera de estos hilos estan presentes en el mensaje,
				TerrariaChatRelay no  transmitirá el mensaje,
				'''
		}
	}

